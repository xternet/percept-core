pragma solidity 0.8.17;

import {Owned} from "solmate/auth/Owned.sol";


contract ZKPVerifier is Owned(msg.sender) {
  constructor() {}

  function verify(bool proof) external pure returns (bool) {
    if(proof) {
      return proof;
    } else {
      revert();
    }
  }

